generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model toadz {
  toad_id   Int @id
  toad_name String @default("")
  owner_id  String @default("")
  vibing    Boolean @default(false)
  level     Int @default(0)
  xp        Int @default(0)
  overall   Int @default(10)
  fed       Int @default(10)
  energy    Int @default(10)
  happiness Int @default(10)
  health    Int @default(10)
  spirit    Int @default(10)
  state     String @default("")
}
